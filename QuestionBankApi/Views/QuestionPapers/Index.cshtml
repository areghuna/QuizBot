@model IEnumerable<QuestionBankApi.Models.QuestionPaper>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.QuestionPaperName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.QuestionPaperDescription)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.QuestionPaperName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.QuestionPaperDescription)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.QuestionPaperId }) |
            @Html.ActionLink("Details", "Details", new { id=item.QuestionPaperId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.QuestionPaperId })
        </td>
    </tr>
}

</table>
